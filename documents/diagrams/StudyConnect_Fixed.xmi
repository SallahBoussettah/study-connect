<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="1.2" xmlns:UML="http://schema.omg.org/spec/UML/1.4">
  <XMI.header>
    <XMI.documentation>
      <XMI.exporter>StarUML</XMI.exporter>
      <XMI.exporterVersion>5.0</XMI.exporterVersion>
    </XMI.documentation>
    <XMI.metamodel xmi.name="UML" xmi.version="1.4"/>
  </XMI.header>
  <XMI.content>
    <UML:Model xmi.id="StudyConnect" name="StudyConnect">
      <UML:Namespace.ownedElement>
        <UML:Model xmi.id="StudyConnectModel" name="StudyConnect Model">
          <UML:Namespace.ownedElement>
            <!-- Use Case Diagram Elements -->
            <UML:Actor xmi.id="Actor_Student" name="Student"/>
            <UML:Actor xmi.id="Actor_Teacher" name="Teacher"/>
            <UML:Actor xmi.id="Actor_Administrator" name="Administrator"/>
            
            <UML:Generalization xmi.id="Gen_TeacherStudent" child="Actor_Teacher" parent="Actor_Student"/>
            <UML:Generalization xmi.id="Gen_AdminTeacher" child="Actor_Administrator" parent="Actor_Teacher"/>
            
            <UML:Package xmi.id="Package_UserManagement" name="User Management">
              <UML:Namespace.ownedElement>
                <UML:UseCase xmi.id="UC1" name="Register Account"/>
                <UML:UseCase xmi.id="UC2" name="Login"/>
                <UML:UseCase xmi.id="UC3" name="Manage Profile"/>
                <UML:UseCase xmi.id="UC4" name="Reset Password"/>
              </UML:Namespace.ownedElement>
            </UML:Package>
            
            <UML:Package xmi.id="Package_StudyRoomManagement" name="Study Room Management">
              <UML:Namespace.ownedElement>
                <UML:UseCase xmi.id="UC5" name="Create Study Room"/>
                <UML:UseCase xmi.id="UC6" name="Join Study Room"/>
                <UML:UseCase xmi.id="UC7" name="Search Study Rooms"/>
                <UML:UseCase xmi.id="UC8" name="Manage Room Members"/>
                <UML:UseCase xmi.id="UC9" name="Configure Room Settings"/>
              </UML:Namespace.ownedElement>
            </UML:Package>
            
            <UML:Package xmi.id="Package_Communication" name="Communication">
              <UML:Namespace.ownedElement>
                <UML:UseCase xmi.id="UC10" name="Send Text Messages"/>
                <UML:UseCase xmi.id="UC11" name="Participate in Audio/Video Call"/>
                <UML:UseCase xmi.id="UC12" name="Share Screen"/>
                <UML:UseCase xmi.id="UC13" name="View Online Users"/>
              </UML:Namespace.ownedElement>
            </UML:Package>
            
            <UML:Package xmi.id="Package_ResourceManagement" name="Resource Management">
              <UML:Namespace.ownedElement>
                <UML:UseCase xmi.id="UC14" name="Upload Resources"/>
                <UML:UseCase xmi.id="UC15" name="Download Resources"/>
                <UML:UseCase xmi.id="UC16" name="Organize Resources"/>
                <UML:UseCase xmi.id="UC17" name="Preview Documents"/>
              </UML:Namespace.ownedElement>
            </UML:Package>
            
            <UML:Package xmi.id="Package_LearningTools" name="Learning Tools">
              <UML:Namespace.ownedElement>
                <UML:UseCase xmi.id="UC18" name="Create Flashcards"/>
                <UML:UseCase xmi.id="UC19" name="Use Study Timer"/>
                <UML:UseCase xmi.id="UC20" name="Track Study Progress"/>
              </UML:Namespace.ownedElement>
            </UML:Package>
            
            <UML:Package xmi.id="Package_Administration" name="Administration">
              <UML:Namespace.ownedElement>
                <UML:UseCase xmi.id="UC21" name="Manage Users"/>
                <UML:UseCase xmi.id="UC22" name="Monitor System"/>
                <UML:UseCase xmi.id="UC23" name="Generate Reports"/>
                <UML:UseCase xmi.id="UC24" name="Moderate Content"/>
              </UML:Namespace.ownedElement>
            </UML:Package>
            
            <!-- Class Diagram Elements -->
            <UML:Package xmi.id="Package_UserManagementClasses" name="User Management">
              <UML:Namespace.ownedElement>
                <UML:Class xmi.id="Class_User" name="User">
                  <UML:Classifier.feature>
                    <UML:Attribute xmi.id="User_id" name="id" visibility="private" type="UUID"/>
                    <UML:Attribute xmi.id="User_firstName" name="firstName" visibility="private" type="String"/>
                    <UML:Attribute xmi.id="User_lastName" name="lastName" visibility="private" type="String"/>
                    <UML:Attribute xmi.id="User_email" name="email" visibility="private" type="String"/>
                    <UML:Attribute xmi.id="User_password" name="password" visibility="private" type="String"/>
                    <UML:Attribute xmi.id="User_role" name="role" visibility="private" type="Enum"/>
                    <UML:Attribute xmi.id="User_avatar" name="avatar" visibility="private" type="String"/>
                    <UML:Attribute xmi.id="User_bio" name="bio" visibility="private" type="Text"/>
                    <UML:Attribute xmi.id="User_institution" name="institution" visibility="private" type="String"/>
                    <UML:Attribute xmi.id="User_major" name="major" visibility="private" type="String"/>
                    <UML:Attribute xmi.id="User_yearOfStudy" name="yearOfStudy" visibility="private" type="String"/>
                    <UML:Attribute xmi.id="User_emailVerified" name="emailVerified" visibility="private" type="Boolean"/>
                    <UML:Attribute xmi.id="User_isActive" name="isActive" visibility="private" type="Boolean"/>
                    <UML:Attribute xmi.id="User_lastLogin" name="lastLogin" visibility="private" type="Date"/>
                    <UML:Attribute xmi.id="User_createdAt" name="createdAt" visibility="private" type="Date"/>
                    <UML:Attribute xmi.id="User_updatedAt" name="updatedAt" visibility="private" type="Date"/>
                    
                    <UML:Operation xmi.id="User_matchPassword" name="matchPassword" visibility="public">
                      <UML:Parameter xmi.id="User_matchPassword_param1" name="password" type="String"/>
                      <UML:Parameter xmi.id="User_matchPassword_return" name="return" type="Boolean" kind="return"/>
                    </UML:Operation>
                    
                    <UML:Operation xmi.id="User_getSignedJwtToken" name="getSignedJwtToken" visibility="public">
                      <UML:Parameter xmi.id="User_getSignedJwtToken_return" name="return" type="String" kind="return"/>
                    </UML:Operation>
                  </UML:Classifier.feature>
                </UML:Class>
                
                <UML:Class xmi.id="Class_UserPreference" name="UserPreference">
                  <UML:Classifier.feature>
                    <UML:Attribute xmi.id="UserPreference_id" name="id" visibility="private" type="UUID"/>
                    <UML:Attribute xmi.id="UserPreference_userId" name="userId" visibility="private" type="UUID"/>
                    <UML:Attribute xmi.id="UserPreference_theme" name="theme" visibility="private" type="String"/>
                    <UML:Attribute xmi.id="UserPreference_notifications" name="notifications" visibility="private" type="Boolean"/>
                    <UML:Attribute xmi.id="UserPreference_emailNotifications" name="emailNotifications" visibility="private" type="Boolean"/>
                    <UML:Attribute xmi.id="UserPreference_soundEffects" name="soundEffects" visibility="private" type="Boolean"/>
                    <UML:Attribute xmi.id="UserPreference_createdAt" name="createdAt" visibility="private" type="Date"/>
                    <UML:Attribute xmi.id="UserPreference_updatedAt" name="updatedAt" visibility="private" type="Date"/>
                  </UML:Classifier.feature>
                </UML:Class>
                
                <UML:Class xmi.id="Class_UserPresence" name="UserPresence">
                  <UML:Classifier.feature>
                    <UML:Attribute xmi.id="UserPresence_id" name="id" visibility="private" type="UUID"/>
                    <UML:Attribute xmi.id="UserPresence_userId" name="userId" visibility="private" type="UUID"/>
                    <UML:Attribute xmi.id="UserPresence_status" name="status" visibility="private" type="String"/>
                    <UML:Attribute xmi.id="UserPresence_lastActive" name="lastActive" visibility="private" type="Date"/>
                    <UML:Attribute xmi.id="UserPresence_device" name="device" visibility="private" type="String"/>
                    <UML:Attribute xmi.id="UserPresence_createdAt" name="createdAt" visibility="private" type="Date"/>
                    <UML:Attribute xmi.id="UserPresence_updatedAt" name="updatedAt" visibility="private" type="Date"/>
                  </UML:Classifier.feature>
                </UML:Class>
              </UML:Namespace.ownedElement>
            </UML:Package>
            
            <UML:Package xmi.id="Package_StudyRoomsClasses" name="Study Rooms">
              <UML:Namespace.ownedElement>
                <UML:Class xmi.id="Class_StudyRoom" name="StudyRoom">
                  <UML:Classifier.feature>
                    <UML:Attribute xmi.id="StudyRoom_id" name="id" visibility="private" type="UUID"/>
                    <UML:Attribute xmi.id="StudyRoom_name" name="name" visibility="private" type="String"/>
                    <UML:Attribute xmi.id="StudyRoom_description" name="description" visibility="private" type="Text"/>
                    <UML:Attribute xmi.id="StudyRoom_image" name="image" visibility="private" type="String"/>
                    <UML:Attribute xmi.id="StudyRoom_totalMembers" name="totalMembers" visibility="private" type="Integer"/>
                    <UML:Attribute xmi.id="StudyRoom_activeMembers" name="activeMembers" visibility="private" type="Integer"/>
                    <UML:Attribute xmi.id="StudyRoom_lastActive" name="lastActive" visibility="private" type="Date"/>
                    <UML:Attribute xmi.id="StudyRoom_isActive" name="isActive" visibility="private" type="Boolean"/>
                    <UML:Attribute xmi.id="StudyRoom_createdBy" name="createdBy" visibility="private" type="UUID"/>
                    <UML:Attribute xmi.id="StudyRoom_subjectId" name="subjectId" visibility="private" type="UUID"/>
                    <UML:Attribute xmi.id="StudyRoom_createdAt" name="createdAt" visibility="private" type="Date"/>
                    <UML:Attribute xmi.id="StudyRoom_updatedAt" name="updatedAt" visibility="private" type="Date"/>
                  </UML:Classifier.feature>
                </UML:Class>
                
                <UML:Class xmi.id="Class_Subject" name="Subject">
                  <UML:Classifier.feature>
                    <UML:Attribute xmi.id="Subject_id" name="id" visibility="private" type="UUID"/>
                    <UML:Attribute xmi.id="Subject_name" name="name" visibility="private" type="String"/>
                    <UML:Attribute xmi.id="Subject_description" name="description" visibility="private" type="Text"/>
                    <UML:Attribute xmi.id="Subject_icon" name="icon" visibility="private" type="String"/>
                    <UML:Attribute xmi.id="Subject_createdAt" name="createdAt" visibility="private" type="Date"/>
                    <UML:Attribute xmi.id="Subject_updatedAt" name="updatedAt" visibility="private" type="Date"/>
                  </UML:Classifier.feature>
                </UML:Class>
              </UML:Namespace.ownedElement>
            </UML:Package>
            
            <!-- Class Relationships -->
            <UML:Association xmi.id="Assoc_UserToUserPreference" name="has">
              <UML:Association.connection>
                <UML:AssociationEnd xmi.id="UserToUserPreference_user" name="" visibility="public" isNavigable="false" aggregation="none" multiplicity="1" participant="Class_User"/>
                <UML:AssociationEnd xmi.id="UserToUserPreference_preference" name="" visibility="public" isNavigable="true" aggregation="none" multiplicity="1" participant="Class_UserPreference"/>
              </UML:Association.connection>
            </UML:Association>
            
            <UML:Association xmi.id="Assoc_UserToUserPresence" name="has">
              <UML:Association.connection>
                <UML:AssociationEnd xmi.id="UserToUserPresence_user" name="" visibility="public" isNavigable="false" aggregation="none" multiplicity="1" participant="Class_User"/>
                <UML:AssociationEnd xmi.id="UserToUserPresence_presence" name="" visibility="public" isNavigable="true" aggregation="none" multiplicity="1" participant="Class_UserPresence"/>
              </UML:Association.connection>
            </UML:Association>
            
            <UML:Association xmi.id="Assoc_UserToStudyRoom" name="creates">
              <UML:Association.connection>
                <UML:AssociationEnd xmi.id="UserToStudyRoom_user" name="" visibility="public" isNavigable="false" aggregation="none" multiplicity="1" participant="Class_User"/>
                <UML:AssociationEnd xmi.id="UserToStudyRoom_room" name="" visibility="public" isNavigable="true" aggregation="none" multiplicity="*" participant="Class_StudyRoom"/>
              </UML:Association.connection>
            </UML:Association>
            
            <UML:Association xmi.id="Assoc_UserJoinsStudyRoom" name="joins">
              <UML:Association.connection>
                <UML:AssociationEnd xmi.id="UserJoinsStudyRoom_user" name="" visibility="public" isNavigable="false" aggregation="none" multiplicity="*" participant="Class_User"/>
                <UML:AssociationEnd xmi.id="UserJoinsStudyRoom_room" name="" visibility="public" isNavigable="true" aggregation="none" multiplicity="*" participant="Class_StudyRoom"/>
              </UML:Association.connection>
            </UML:Association>
            
            <UML:Association xmi.id="Assoc_StudyRoomToSubject" name="belongs to">
              <UML:Association.connection>
                <UML:AssociationEnd xmi.id="StudyRoomToSubject_room" name="" visibility="public" isNavigable="false" aggregation="none" multiplicity="*" participant="Class_StudyRoom"/>
                <UML:AssociationEnd xmi.id="StudyRoomToSubject_subject" name="" visibility="public" isNavigable="true" aggregation="none" multiplicity="1" participant="Class_Subject"/>
              </UML:Association.connection>
            </UML:Association>
            
            <!-- Sequence Diagram Elements -->
            <UML:Collaboration xmi.id="Collaboration_MessagingSequence" name="StudyConnect Messaging Sequence">
              <UML:Namespace.ownedElement>
                <UML:ClassifierRole xmi.id="Role_User" name="User" isSpecification="false"/>
                <UML:ClassifierRole xmi.id="Role_FrontendReactApp" name="Frontend React App" isSpecification="false"/>
                <UML:ClassifierRole xmi.id="Role_BackendAPIServer" name="Backend API Server" isSpecification="false"/>
                <UML:ClassifierRole xmi.id="Role_SocketIOServer" name="Socket.IO Server" isSpecification="false"/>
                <UML:ClassifierRole xmi.id="Role_Database" name="Database" isSpecification="false"/>
                <UML:ClassifierRole xmi.id="Role_OtherUsersInRoom" name="Other Users in Room" isSpecification="false"/>
              </UML:Namespace.ownedElement>
            </UML:Collaboration>
            
            <!-- Component Diagram Elements -->
            <UML:Package xmi.id="Package_Frontend" name="Frontend (React)">
              <UML:Namespace.ownedElement>
                <UML:Component xmi.id="Component_LandingPages" name="Landing Pages"/>
                <UML:Component xmi.id="Component_AuthenticationUI" name="Authentication UI"/>
                <UML:Component xmi.id="Component_Dashboard" name="Dashboard"/>
                <UML:Component xmi.id="Component_StudyRoomUI" name="Study Room UI"/>
                <UML:Component xmi.id="Component_ResourceManager" name="Resource Manager"/>
                <UML:Component xmi.id="Component_ChatInterface" name="Chat Interface"/>
                <UML:Component xmi.id="Component_LearningToolsUI" name="Learning Tools UI"/>
                <UML:Component xmi.id="Component_ReactRouter" name="React Router"/>
                <UML:Component xmi.id="Component_ContextAPI" name="Context API"/>
                <UML:Component xmi.id="Component_SocketIOClient" name="Socket.IO Client"/>
                <UML:Component xmi.id="Component_AxiosHTTPClient" name="Axios HTTP Client"/>
              </UML:Namespace.ownedElement>
            </UML:Package>
            
            <UML:Package xmi.id="Package_Backend" name="Backend (Node.js/Express)">
              <UML:Namespace.ownedElement>
                <UML:Component xmi.id="Component_APIRoutes" name="API Routes"/>
                <UML:Component xmi.id="Component_Controllers" name="Controllers"/>
                <UML:Component xmi.id="Component_Models" name="Models"/>
                <UML:Component xmi.id="Component_Middleware" name="Middleware"/>
                <UML:Component xmi.id="Component_SocketIOServer" name="Socket.IO Server"/>
                <UML:Component xmi.id="Component_FileUploadService" name="File Upload Service"/>
                <UML:Component xmi.id="Component_AuthenticationService" name="Authentication Service"/>
              </UML:Namespace.ownedElement>
            </UML:Package>
            
            <UML:Artifact xmi.id="Artifact_PostgreSQL" name="PostgreSQL" stereotype="database"/>
            <UML:Interface xmi.id="Interface_RESTfulAPI" name="RESTful API"/>
            <UML:Interface xmi.id="Interface_WebSocket" name="WebSocket"/>
            
            <!-- Deployment Diagram Elements -->
            <UML:Node xmi.id="Node_ClientDevice" name="Client Device">
              <UML:Namespace.ownedElement>
                <UML:Artifact xmi.id="Artifact_WebBrowser" name="Web Browser"/>
              </UML:Namespace.ownedElement>
            </UML:Node>
            
            <UML:Node xmi.id="Node_FrontendServer" name="Frontend Server">
              <UML:Namespace.ownedElement>
                <UML:Artifact xmi.id="Artifact_NginxWebServer" name="Nginx Web Server"/>
                <UML:Artifact xmi.id="Artifact_ReactApplication" name="React Application"/>
                <UML:Artifact xmi.id="Artifact_StaticAssets" name="Static Assets"/>
              </UML:Namespace.ownedElement>
            </UML:Node>
            
            <UML:Node xmi.id="Node_APIServer" name="API Server">
              <UML:Namespace.ownedElement>
                <UML:Artifact xmi.id="Artifact_NodeJS" name="Node.js"/>
                <UML:Artifact xmi.id="Artifact_ExpressJS" name="Express.js"/>
                <UML:Artifact xmi.id="Artifact_SocketIOServer" name="Socket.IO Server"/>
              </UML:Namespace.ownedElement>
            </UML:Node>
            
            <UML:Node xmi.id="Node_DatabaseServer" name="Database Server">
              <UML:Namespace.ownedElement>
                <UML:Artifact xmi.id="Artifact_PostgreSQLDB" name="PostgreSQL" stereotype="database"/>
              </UML:Namespace.ownedElement>
            </UML:Node>
            
            <UML:Node xmi.id="Node_CloudServices" name="Cloud Services" stereotype="cloud">
              <UML:Namespace.ownedElement>
                <UML:Artifact xmi.id="Artifact_ObjectStorage" name="Object Storage"/>
                <UML:Artifact xmi.id="Artifact_EmailService" name="Email Service"/>
                <UML:Artifact xmi.id="Artifact_CDN" name="CDN"/>
              </UML:Namespace.ownedElement>
            </UML:Node>
            
            <!-- Diagrams -->
            <UML:UseCaseDiagram xmi.id="UseCaseDiagram_Main" name="StudyConnect Use Case Diagram" diagramType="UseCaseDiagram"/>
            <UML:ClassDiagram xmi.id="ClassDiagram_Main" name="StudyConnect Class Diagram" diagramType="ClassDiagram"/>
            <UML:SequenceDiagram xmi.id="SequenceDiagram_Messaging" name="StudyConnect Messaging Sequence" diagramType="SequenceDiagram"/>
            <UML:ComponentDiagram xmi.id="ComponentDiagram_Main" name="StudyConnect Component Diagram" diagramType="ComponentDiagram"/>
            <UML:DeploymentDiagram xmi.id="DeploymentDiagram_Main" name="StudyConnect Deployment Diagram" diagramType="DeploymentDiagram"/>
          </UML:Namespace.ownedElement>
        </UML:Model>
      </UML:Namespace.ownedElement>
    </UML:Model>
  </XMI.content>
</XMI> 